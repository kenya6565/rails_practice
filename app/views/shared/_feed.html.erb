<% if @feed_items.any? %>
  <ol class="microposts">
      <!--ここは@feed_itemsの各要素がMicropostクラスを持っていたため-->
      <!--RailsはMicropostのパーシャルを呼び出している(_micropost.html.erb)-->
    <%= render @feed_items %>
  </ol>
  <%= will_paginate @feed_items,params: { controller: :static_pages, action: :home } %>
<% end %>